    "install": "^0.13.0",
    "link": "^1.5.1",
    "nativewind": "^2.0.7",
    "react": "18.0.0",
    "react-dom": "^18.0.0",
    "react-native": "^0.69.5",
    "react-native-axios": "^0.17.1",
    "react-native-heroicons": "^3.2.0",
